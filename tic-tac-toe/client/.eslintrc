{
    "parser": "babel-eslint",
    "env": {
		"browser": true,
		"es6": true,
		"commonjs": true,
		"jest": true
	},
    "extends": "airbnb",
    "plugins": [
        "react-hooks"
    ],
    "rules": {
        "indent": ["error", 4],
        "react/jsx-indent": ["error", 4],
        "react/jsx-indent-props": ["error", "space", 4],
        "linebreak-style": ["error", "windows"],
        "react-hooks/rules-of-hooks": "error"
    }
}